            <result property="region" column="region" jdbcType="VARCHAR"/>
            <result property="city" column="city" jdbcType="VARCHAR"/>
            <result property="qsrank" column="qsrank" jdbcType="INTEGER"/>
            <result property="rkrank" column="rkrank" jdbcType="INTEGER"/>
